<% namespace admin_view %>
<% class admin_cache uses data::admin_cache extends admin_base %>
<% template master_content() %>
<% if cache_enabled %>
	<p><% ngt "Cached one entry.","Cached %1% entries.",cache_keys using cache_keys %></p>
	<p>
		<form action="<% submit_url %>" method="post">
		<input type="submit" name="clear" value="<% gt "Clear Cache" %>" />
		</form>
	</p>
<% else %>
	<p><% gt "The caching system is disabled." %></p>
<% end %>
<% end template%>
<% end class %>
<% end namespace %>
